#:kivy 2.1.0
<HomeScreen>:
    name: 'home'

    FloatLayout:
        Image:
            source: 'resources/images/home.jpeg'
            allow_stretch: True
            keep_ratio: False
            size: self.parent.size
            pos: self.parent.pos

        Button:
            background_normal: 'resources/images/start.jpeg'
            background_down: 'resources/images/start.jpeg'
            size_hint: .2, .15
            pos_hint: {'center_x': .5, 'center_y': .25}
            on_release:
                app.root.current = 'story'